        "slim/flash": "^0.4.0",
        "symfony/console": "^5.2",
        "league/flysystem": "^2.0"
    },
    "require-dev" : {
        "symfony/var-dumper" : "^5.2",
        "phpunit/phpunit": "^9.5",
